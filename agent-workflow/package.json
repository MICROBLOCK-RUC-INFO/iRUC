{
    "name": "workflow-project",
    "version": "1.0.0",
    "description": "基于Flowise和LangChain.js的智能体工作流项目",
    "main": "server/index.js",
    "scripts": {
        "start": "node server/index.js",
        "dev": "nodemon server/index.js",
        "test": "jest",
        "init": "node scripts/init-project.js",
        "build": "npm run init && npm run build:grammar",
        "build:grammar": "node grammar/build-parser.js",
        "flowise": "node scripts/start-flowise.js",
        "flowise:simple": "npx flowise start",
        "web": "npm start",
        "clean": "rm -rf node_modules package-lock.json",
        "reset": "npm run clean && npm install && npm run build"
    },
    "dependencies": {
    "express": "^4.18.2",
    "langchain": "^0.2.0",
    "@langchain/openai": "^0.2.0",
    "@langchain/anthropic": "^0.2.0",
    "@langchain/google-genai": "^0.2.0",
    "flowise": "^1.8.0",
    "pegjs": "^0.10.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "ws": "^8.16.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "sqlite3": "^5.1.6"
    },
    "devDependencies": {
      "nodemon": "^3.1.4",
      "jest": "^29.7.0"
    },
    "keywords": [
      "langchain",
      "flowise",
      "workflow",
      "ai",
      "agents"
    ],
    "author": "Workflow Team",
    "license": "MIT"
  }
  